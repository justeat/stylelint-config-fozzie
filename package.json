{
  "name": "@justeat/stylelint-config-fozzie",
  "version": "2.1.0",
  "description": "Shareable Stylelint config for use with Fozzie – Just Eat’s UI Component Framework",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "standard"
  ],
  "author": "stylelint",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/justeat/stylelint-config-fozzie"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "@justeat/eslint-config-fozzie": "1.0.0",
    "danger": "3.0.2",
    "eslint": "4.2.0",
    "eslint-plugin-import": "2.2.0",
    "jest": "22.0.5",
    "npm-run-all": "4.0.0",
    "npmpub": "3.0.1",
    "remark-cli": "4.0.0",
    "remark-preset-lint-recommended": "3.0.1",
    "stylelint": "13.2.0",
    "stylelint-order": "4.0.0",
    "stylelint-scss": "3.14.2"
  },
  "peerDependencies": {
    "stylelint": "8.0.0",
    "stylelint-scss": "2.2.0"
  },
  "scripts": {
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:md": "remark . --quiet --frail",
    "lint": "npm-run-all --parallel lint:*",
    "pretest": "yarn run lint",
    "release": "npmpub",
    "test": "jest",
    "watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  },
  "remarkConfig": {
    "plugins": [
      "preset-lint-recommended"
    ]
  }
}
